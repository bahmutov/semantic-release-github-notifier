{
  "author": {
    "name": "Hutson Betts",
    "email": "hutson@hyper-expanse.net",
    "url": "https://github.com/hbetts"
  },
  "bin": {},
  "bugs": "https://github.com/hbetts/semantic-release-github-notifier/issues",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "semantic-release plugin that notifies GitHub issues and pull requests of a related package release.",
  "engines": {
    "node": "^4.1.0 || ^0.12.0",
    "npm": "^3.0.0 || ^2.1.9"
  },
  "files": [
    "server/"
  ],
  "keywords": [
    "semantic-release",
    "github"
  ],
  "license": "Apache-2.0",
  "main": "./src/index.js",
  "name": "semantic-release-github-notifier",
  "release": {
    "verifyConditions": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hbetts/semantic-release-github-notifier.git"
  },
  "scripts": {
    "docs": "gulp jsdoc",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post",
    "start": "node ./server/serve.js",
    "test": "gulp"
  },
  "version": "1.0.0",
  "dependencies": {
    "git-head": "^1.7.0",
    "github": "^0.2.4",
    "parse-github-repo-url": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "codecov": "^1.0.1",
    "cz-conventional-changelog": "^1.1.4",
    "del": "^2.0.2",
    "gulp": "^3.4.0",
    "gulp-istanbul": "^0.10.0",
    "gulp-jscs": "^3.0.0",
    "gulp-jsdoc": "^0.1.4",
    "gulp-jshint": "^2.0.0",
    "gulp-mocha": "^2.0.0",
    "gulp-watch": "^4.3.5",
    "jsdoc": "^3.3.3",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "nock": "^3.0.0",
    "semantic-release": "^4.3.5",
    "tmp": "0.0.28"
  }
}
